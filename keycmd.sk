command /key [<text>] [<text>]:
	permission: key.get
	permission message: &7[&6&lLuxury&c&lCraft&7] &cNo permission!
	trigger:
		if arg-1 is "party":
			execute console command "crate key %arg-1% party %arg-2%"
			send "&7[&6&lLuxury&c&lCrates&7] &aKeys Sent!"
			stop
		if arg-1 is "common":
			execute console command "crate key %player% common %arg-2%"
			send "&7[&6&lLuxury&c&lCrates&7] &aKeys Sent!"
			stop
