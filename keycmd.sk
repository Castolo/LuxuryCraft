			send "&7[&6&lLuxury&c&lCrates&7] &aKeys Sent!"
			stop
		if arg-1 is "rainbow":
			execute console command "crate key %player% rainbow %arg-2%"
			send "&7[&6&lLuxury&c&lCrates&7] &aKeys Sent!"
			stop
