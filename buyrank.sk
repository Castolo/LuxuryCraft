command /bre [<text>]:
	executable by: Players
	trigger:
		if arg-1 is "vip+":
			if player's balance is above or equal to 10000000:
				if player does not have permission "vip+.higher":
					send "&aSuccessfully purchased!"
					execute console command "/manuadd %player% vip+"
					execute console command "/eco take %player% 10000000"
				else:
					send "&7You already have a higher rank!"
			else:
				send "&7You have insufficient funds!"
		if arg-1 is "mvp+":
			if player's balance is above or equal to 25000000:
				if player does not have permission "mvp+.higher":
					send "&aSuccessfully purchased!"
					execute console command "/manuadd %player% mvp+"
					execute console command "/eco take %player% 25000000"
				else:
					send "&7You already have a higher rank!"
			else:
				send "&7You have insufficient funds!"
		if arg-1 is "mvp++":
			if player's balance is above or equal to 50000000:
				if player does not have permission "mvp++.higher":
					send "&aSuccessfully purchased!"
					execute console command "/manuadd %player% mvp++"
					execute console command "/eco take %player% 50000000"
				else:
					send "&7You already have a higher rank!"
			else:
				send "&7You have insufficient funds!"