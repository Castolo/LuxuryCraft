command /buyrank [<text>]:
	executable by: Players
	aliases: /br
	trigger:
		if arg-1 is not set:
			open chest with 1 row named "&8&lRanks" to player
			format slot 3 of player with paper named "&2&lVIP&6&l+ &fRank" with lore "&aPrice: $10.000.000" to run [execute player command "/buyrank vip+"]
			format slot 4 of player with paper named "&b&lMVP&6&l+ &fRank" with lore "&aPrice: $25.000.000" to run [execute player command "/buyrank mvp+"]
			format slot 5 of player with paper named "&b&lMVP&6&l++ &fRank" with lore "&aPrice: $50.000.000" to run [execute player command "/buyrank mvp++"]
		if arg-1 is "vip+":
			execute player command "/brconfirm vip+"
		if arg-1 is "mvp+":
			execute player command "/brconfirm mvp+"
		if arg-1 is "mvp++":
			execute player command "/brconfirm mvp++"
			
